<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.webdev.dto">

	<class name="BugDTO" table="bugs">

		<id name="id" column="ID">
			<generator class="increment" />
		</id>
		<property name="summary" column="SUMMARY" />
		<property name="description" column="DESCRIPTION" />
		<!-- property name="priorityId" column="PRIORITY_ID" /-->
		<!-- property name="typeId" column="TYPE_ID" /-->
		<property name="statusId" column="STATUS_ID" />
		<!-- property name="moduleId" column="MODULE_ID" /-->
		<!-- property name="estimatedDays" column="ESTIMATED_DAYS" />
		<property name="assignedToUserId" column="ASSIGN_TO_USER_ID" />
		<property name="authorUserId" column="AUTHOR_USER_ID" /-->

	</class>

</hibernate-mapping>
